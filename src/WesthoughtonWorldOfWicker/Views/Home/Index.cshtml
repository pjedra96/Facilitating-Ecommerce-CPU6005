@model IEnumerable<WesthoughtonWorldOfWicker.Models.Category>
@{
    ViewData["Title"] = "Home Page";
}
<div id="myCarousel" class="carousel slide" data-ride="carousel" data-interval="6000">
    <ol class="carousel-indicators">
        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="1"></li>
        <li data-target="#myCarousel" data-slide-to="2"></li>
        <li data-target="#myCarousel" data-slide-to="3"></li>
    </ol>
    <div class="carousel-inner" role="listbox">
        <div class="item active">
            <img src="~/images/wickerindian.jpg" alt="indian wicker furniture set" class="img-responsive" style="width:100%;" />
            <div class="carousel-caption" role="option">
                <p>
                    In order to see more of our wicker furniture, visit the above links
                </p>
            </div>
        </div>
        <div class="item">
            <img src="~/images/cane-furniture-online-in-india.jpg" alt="cane wicker furniture" class="img-responsive" style="width:100%;" />
            <div class="carousel-caption" role="option">
            </div>
        </div>
        <div class="item">
            <img src="~/images/outdoor-patio-dining-table-louisvuitton.jpg" alt="wicker dining room" class="img-responsive" style="width:100%;" />
            <div class="carousel-caption" role="option">
            </div>
        </div>
        <div class="item">
            <img src="~/images/wicker-rattan-furniture-set-wicker.jpg" alt="Visual Studio" class="img-responsive" style="width:100%;"/>
            <div class="carousel-caption" role="option">
            </div>
        </div>
    </div>
    <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
        <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
        <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
    </a>
</div>
<div class="container">
    <br />
    <h4 id="brandNew">Brand New Furniture</h4>
    <br />
    <div class="row">
        <div class="col-md-3" id="centerimg">
            <img src="~/images/wickerindianchair.jpg" alt="IndianWickerChair" width="170" height="210"/>
            <p>Indian Rattan Dining Room Chair</p>
        </div>
        <div class="col-md-3" id="centerimg">
            <img src="~/images/aluminiumwickerchair.jpg" alt="AluminiumIndianWickerChair" width="170" height="210" />
            <p>Aluminium Wicker Chair</p>
        </div>
        <div class="col-md-3" id="centerimg">
            <img src="~/images/wickercoffeetable.jpg" alt="BlackWickerCoffeeTable" width="170" height="210" />
            <p>Black Wicker Cofee Table</p>
        </div>
        <div class="col-md-3" id="centerimg">
            <img src="~/images/Rattan-Small-Conservatory-Side-Table-Ankara.jpg" alt="ConservatorySideTable" width="170" height="210" />
            <p>Cane Coffee Table (Glass Table Top)</p>
        </div>

        <div class="col-md-3" id="centerimg">
            <img src="~/images/FB-3728-E-WICKER-SIDE-TABLE1.jpg" alt="RattanSideTable" width="170" height="210" />
            <p>Rattan Side Table</p>
        </div>
        <div class="col-md-3" id="centerimg">
            <img src="~/images/elegant-unique-rattan-round-coffee-table.jpg" alt="RattanRoundCoffeeTable" width="170" height="210" />
            <p>Elegant Unique Rattan Round Coffee Table</p>
        </div>
        <div class="col-md-3" id="centerimg">
            <img src="~/images/Wicker_Lift-off_Lid_Storage_Basket.jpg" alt="Lift-OffLidStorageBasket" width="170" height="210" />
            <p>Lift-Off Lid Wicker Storage Basket</p>
        </div>
        <div class="col-md-3" id="centerimg">
            <img src="~/images/img-thing.jpg" alt="WickerResinCircularChair" width="170" height="210" />
            <p>Wicker Teak Furniture Resin Circular Chair</p>
        </div>
    </div>
    <br />
</div>
<br />
<div class="row">
    <div class="row" style="background-color:darkblue; color:white;">
        <div class="col-lg-12">
            <h3 class="text-center">INSPIRATION BOARD</h3><br/>
        </div>
        <div class="col-md-6">
            <img id="inspirationImg" src="~/images/Unique-Wicker-Dining-Room-Chairs.jpg" />
            <h3 class="text-center">Dining Room</h3>
        </div>
        <div class="col-md-6">
            <img id="inspirationImg" src="~/images/clever-design-rattan-living-room-furniture.jpg"  />
            <h3 class="text-center">Living Room</h3>
        </div>
        <div class="col-md-6">
            <br />
            <img id="inspirationImg" src="~/images/Sersley-690x466.jpg" />
            <h3 class="text-center">Conservatory</h3>
        </div>
        <div class="col-md-6">
            <br />
            <img id="inspirationImg" src="~/images/rattan-garde-furniture.jpg"/>
            <h3 class="text-center">Garden</h3>
        </div>
    </div>
</div>
<br />