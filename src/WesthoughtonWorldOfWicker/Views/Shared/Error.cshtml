@{
    ViewData["Title"] = "Error";
}

<h2 class="text-danger text-center">Error !</h2>
<h3 class="text-danger text-center">An error occurred while processing your request.</h3>
<p class="text-center"> We're sorry, we've hit an unexpected error.
    <a href="javascript:history.go(-1)">Click here</a>
    if you'd like to go back and try that again.
</p>
<hr/>
<h4 class="text-warning text-center">@ViewBag.ErrorMessage</h4>
