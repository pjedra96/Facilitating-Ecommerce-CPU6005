@model IEnumerable<WesthoughtonWorldOfWicker.Models.Category>

@{
    ViewData["Title"] = "Store";
}

<h3>Browse Categories</h3>
<p> Select from @Model.Count() categories:</p>
<ul class="list-group">
    @foreach (var category in Model)
    {
        <li class="list-group-item"><a asp-controller="Store" asp-action="Browse" asp-route-category="@category.CategoryName">@category.CategoryName</a></li>
    }
</ul>
